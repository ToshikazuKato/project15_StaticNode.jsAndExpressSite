extends layout.pug

block content
  article.grid-container.portfolio-intro
    div

      h1 My Portfolio

      p.lead.text-light
        | A list of projects from Fullstack Javascript Techdegree
  article.grid-container.portfolio-index

    .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3

      each prj in projects
        .cell
          a(href=`${prj.live_link}` target="_blank")
            img.thumbnail(src=`${prj.images_urls[0]}`)
            h5 #{prj.project_name}
